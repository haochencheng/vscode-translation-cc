{
  "name": "translate-selected",
  "displayName": "Translate-selected",
  "description": "Translate selected text with Google/DeepL/Youdao providers.",
  "version": "0.0.1",
  "publisher": "cliff",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:translate-selected.translateSelection",
    "onCommand:translate-selected.translateInput",
    "onCommand:extension.translateText"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "translate-selected.translateSelection",
        "title": "Translate Selected"
      },
      {
        "command": "translate-selected.translateInput",
        "title": "Translate Input"
      },
      {
        "command": "extension.translateText",
        "title": "Translate Selected (Alias)"
      }
    ],
    "keybindings": [
      {
        "command": "translate-selected.translateSelection",
        "key": "ctrl+shift+t",
        "mac": "cmd+4",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "translate-selected.translateSelection",
          "when": "editorHasSelection"
        }
      ]
    },
    "configuration": {
      "title": "Translate-selected",
      "properties": {
        "translate-selected.provider": {
          "type": "string",
          "enum": ["google", "deepl", "youdao"],
          "default": "google",
          "description": "Translation provider to use."
        },
        "translate-selected.sourceLanguage": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto", "zh-CN", "zh-TW", "en", "ja", "ko", "fr", "de", "es", "pt", "pt-BR",
            "ru", "it", "nl", "pl", "ar", "th", "vi", "id", "ms", "tr", "uk", "cs",
            "da", "fi", "el", "hu", "sv", "nb", "ro", "sk", "bg", "et", "lv", "lt", "sl", "hr"
          ],
          "enumItemLabels": [
            "Auto Detect (自动检测)", "Chinese Simplified (简体中文)", "Chinese Traditional (繁體中文)",
            "English (英语)", "Japanese (日语)", "Korean (韩语)", "French (法语)", "German (德语)",
            "Spanish (西班牙语)", "Portuguese (葡萄牙语)", "Portuguese-Brazil (巴西葡语)",
            "Russian (俄语)", "Italian (意大利语)", "Dutch (荷兰语)", "Polish (波兰语)",
            "Arabic (阿拉伯语)", "Thai (泰语)", "Vietnamese (越南语)", "Indonesian (印尼语)",
            "Malay (马来语)", "Turkish (土耳其语)", "Ukrainian (乌克兰语)", "Czech (捷克语)",
            "Danish (丹麦语)", "Finnish (芬兰语)", "Greek (希腊语)", "Hungarian (匈牙利语)",
            "Swedish (瑞典语)", "Norwegian (挪威语)", "Romanian (罗马尼亚语)", "Slovak (斯洛伐克语)",
            "Bulgarian (保加利亚语)", "Estonian (爱沙尼亚语)", "Latvian (拉脱维亚语)",
            "Lithuanian (立陶宛语)", "Slovenian (斯洛文尼亚语)", "Croatian (克罗地亚语)"
          ],
          "description": "Source language (use 'auto' to detect)."
        },
        "translate-selected.targetLanguage": {
          "type": "string",
          "default": "zh-CN",
          "enum": [
            "zh-CN", "zh-TW", "en", "ja", "ko", "fr", "de", "es", "pt", "pt-BR",
            "ru", "it", "nl", "pl", "ar", "th", "vi", "id", "ms", "tr", "uk", "cs",
            "da", "fi", "el", "hu", "sv", "nb", "ro", "sk", "bg", "et", "lv", "lt", "sl", "hr"
          ],
          "enumItemLabels": [
            "Chinese Simplified (简体中文)", "Chinese Traditional (繁體中文)",
            "English (英语)", "Japanese (日语)", "Korean (韩语)", "French (法语)", "German (德语)",
            "Spanish (西班牙语)", "Portuguese (葡萄牙语)", "Portuguese-Brazil (巴西葡语)",
            "Russian (俄语)", "Italian (意大利语)", "Dutch (荷兰语)", "Polish (波兰语)",
            "Arabic (阿拉伯语)", "Thai (泰语)", "Vietnamese (越南语)", "Indonesian (印尼语)",
            "Malay (马来语)", "Turkish (土耳其语)", "Ukrainian (乌克兰语)", "Czech (捷克语)",
            "Danish (丹麦语)", "Finnish (芬兰语)", "Greek (希腊语)", "Hungarian (匈牙利语)",
            "Swedish (瑞典语)", "Norwegian (挪威语)", "Romanian (罗马尼亚语)", "Slovak (斯洛伐克语)",
            "Bulgarian (保加利亚语)", "Estonian (爱沙尼亚语)", "Latvian (拉脱维亚语)",
            "Lithuanian (立陶宛语)", "Slovenian (斯洛文尼亚语)", "Croatian (克罗地亚语)"
          ],
          "description": "Target language code."
        },
        "translate-selected.google.apiKey": {
          "type": "string",
          "default": "",
          "description": "Google Translation API key."
        },
        "translate-selected.deepl.apiKey": {
          "type": "string",
          "default": "",
          "description": "DeepL API key."
        },
        "translate-selected.deepl.apiUrl": {
          "type": "string",
          "default": "https://api-free.deepl.com/v2/translate",
          "description": "DeepL API endpoint. Use https://api.deepl.com/v2/translate for Pro."
        },
        "translate-selected.youdao.appKey": {
          "type": "string",
          "default": "",
          "description": "Youdao appKey."
        },
        "translate-selected.youdao.appSecret": {
          "type": "string",
          "default": "",
          "description": "Youdao appSecret."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "echo \"No lint configured\""
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
